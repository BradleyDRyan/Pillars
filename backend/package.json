{
  "name": "squirrel2-backend",
  "version": "2.0.0",
  "description": "Node.js backend for Squirrel 2.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:admin": "concurrently -n api,admin -c blue,magenta \"npm run dev\" \"npm --prefix admin-ui run dev\"",
    "test": "jest",
    "admin:build": "npm --prefix admin-ui install && npm --prefix admin-ui run build && rm -rf public/connection-admin && mkdir -p public/connection-admin && cp -R admin-ui/dist/* public/connection-admin/",
    "vercel-build": "echo 'Skipping admin build'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "@upstash/qstash": "^2.8.4",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "cron-parser": "^5.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^11.11.0",
    "google-gax": "^4.0.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.15.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "protobufjs": "^7.2.0",
    "twilio": "^5.10.7",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": "20.x"
  },
  "author": "",
  "license": "MIT"
}
